{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build-themes && ng serve",
    "build": "npm run build-themes && ng build",
    "build-prod": "npm run build-themes && ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "build-themes": "bash ./tools/build-themes.sh",
    "e2e": "ng e2e --protractor-config=e2e/protractor-ci.conf.js --configuration production",
    "dev:ssr": "npm run build-themes && ng run frontend:serve-ssr",
    "serve:ssr": "node dist/frontend/server/main.js",
    "build:ssr": "ng build && ng run frontend:server",
    "build-prod:ssr": "ng build --configuration production && ng run frontend:server:production",
    "prerender": "ng run frontend:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.16",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "^12.2.16",
    "@angular/compiler": "^12.2.16",
    "@angular/core": "^12.2.16",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^12.2.16",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "^12.2.16",
    "@angular/platform-browser-dynamic": "^12.2.16",
    "@angular/platform-server": "^12.2.16",
    "@angular/router": "^12.2.16",
    "@auth0/angular-jwt": "^5.0.2",
    "@nguniversal/express-engine": "^11.2.1",
    "@sentry/browser": "^6.17.3",
    "compression": "^1.7.4",
    "core-js": "^3.8.3",
    "domino": "^2.1.6",
    "express": "^4.15.2",
    "lodash": "^4.17.21",
    "ngx-markdown": "^11.1.3",
    "ngx-mat-select-search": "^3.3.0",
    "ngx-monaco-editor": "^9.0.0",
    "numeral": "^2.0.6",
    "query-string": "^7.1.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.0",
    "yaml": "2.0.0-6",
    "zone.js": "~0.11.4"
